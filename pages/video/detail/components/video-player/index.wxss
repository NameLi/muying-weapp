.video-wrapper {
  width: 100%;
  height: 422rpx;
  background-color: #000;
}

.video-wrapper .video-box {
  z-index: 98;
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 422rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #000;
}

.video-wrapper .video-box .video {
  width: 100%;
  height: 422rpx;
}

.video-wrapper .video-box .progress-wrapper {
  z-index: 99;
  height: 3rpx;
  background: #dedede;
}

.video-wrapper .video-box .progress-wrapper .progress {
  height: 3rpx;
  background: #51ace6;
}
